{
  "name": "nexus_wallet",
  "productName": "Nexus Wallet",
  "version": "1.0.1",
  "moduleSpecVersion": "0.1.0",
  "supportedModuleSpecVersion": "0.1.0",
  "buildDate": "June 28th 2019",
  "description": "User interface for the Nexus blockchain and ecossystem.",
  "scripts": {
    "build": "concurrently \"npm run build-main\" \"npm run build-renderer\" \"npm run build-preload\"",
    "build-dll": "cross-env NODE_ENV=development node --trace-warnings -r @babel/register ./node_modules/webpack/bin/webpack --mode=development --config webpack.config.renderer.dev.dll.js --color",
    "build-main": "cross-env NODE_ENV=production node --trace-warnings -r @babel/register ./node_modules/webpack/bin/webpack --mode=production --config webpack.config.main.prod.js --color",
    "build-main-dev": "cross-env NODE_ENV=development node --trace-warnings -r @babel/register ./node_modules/webpack/bin/webpack --mode=development --config webpack.config.main.dev.js --color",
    "build-renderer": "cross-env NODE_ENV=production node  --max-old-space-size=4076 --trace-warnings -r @babel/register ./node_modules/webpack/bin/webpack --mode=production --config webpack.config.renderer.prod.js --color",
    "build-preload-dev": "cross-env NODE_ENV=development node --trace-warnings -r @babel/register ./node_modules/webpack/bin/webpack --mode=production --config webpack.config.preload.dev.js --color",
    "build-preload": "cross-env NODE_ENV=production node --trace-warnings -r @babel/register ./node_modules/webpack/bin/webpack --mode=production --config webpack.config.preload.prod.js --color",
    "dev": "cross-env START_HOT=1 npm run start-renderer-dev",
    "package": "npm run build && build --publish never",
    "package-all": "npm run build && build -mwl",
    "package-linux": "npm run build && build --linux",
    "package-mac": "npm run build && build --mac",
    "package-win": "npm run build && build --win --x64",
    "preprod": "cross-env NODE_ENV_RUN=production_test npm run build",
    "prod-test": "cross-env NODE_ENV=production NODE_ENV_RUN=production-test electron ./app/",
    "start": "npm run dev",
    "start-main-dev": "cross-env HOT=1 NODE_ENV=development electron ./app/main.dev",
    "start-renderer-dev": "cross-env NODE_ENV=development node --trace-warnings -r @babel/register ./node_modules/webpack-dev-server/bin/webpack-dev-server --config webpack.config.renderer.dev.js",
    "run-tests": "./node_modules/mocha/bin/mocha tests/test.js",
    "version2": "node ./internals/scripts/UpdateBuildDate.js",
    "extract-changes": "node ./internals/scripts/extractTranslationChanges.js",
    "update-translations": "node ./internals/scripts/updateTranslations.js",
    "update-documentation": "./UpdateDocumentation.sh"
  },
  "browserslist": "electron 1.8",
  "build": {
    "productName": "Nexus Wallet",
    "appId": "com.nexusearth.NexusTritium",
    "files": [
      "dist/",
      "node_modules/",
      "app.html",
      "main.prod.js",
      "main.prod.js.map",
      "package.json",
      "resources/**/*",
      "installerSiderbar.bmp"
    ],
    "extraResources": [
      "./app/GeoLite2-City/**/*",
      "./app/images/tray/**/*",
      "./app/cores/**/*",
      "./app/images/background/**/*",
      "license.txt",
      "installerSidebar.bmp",
      "./app/languages/*",
      {
        "from": "./app/fonts",
        "to": "fonts"
      }
    ],
    "publish": {
      "provider": "github",
      "owner": "Nexusoft",
      "repo": "NexusInterface"
    },
    "mac": {
      "target": [
        "dmg",
        "zip"
      ],
      "category": "public.app-category.utilities"
    },
    "dmg": {
      "artifactName": "nexus_wallet-MacOSX-${version}.${ext}",
      "background": "resources/background.jpg",
      "contents": [
        {
          "x": 129,
          "y": 250
        },
        {
          "x": 410,
          "y": 247,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": [
        "nsis"
      ]
    },
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "runAfterFinish": true,
      "allowToChangeInstallationDirectory": true,
      "menuCategory": "Nexus",
      "installerSidebar": "resources/installerSidebar.bmp",
      "license": "resources/license.txt",
      "shortcutName": "Nexus Wallet",
      "artifactName": "nexus_wallet-setup-Win-${version}.${ext}"
    },
    "linux": {
      "target": [
        "deb",
        "AppImage"
      ],
      "category": "P2P"
    },
    "deb": {
      "artifactName": "nexus_wallet-Linux-${version}.${ext}"
    },
    "appImage": {
      "artifactName": "nexus_wallet-Linux-${version}.${ext}"
    },
    "directories": {
      "buildResources": "resources",
      "output": "release"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Nexusoft/NexusInterface.git"
  },
  "author": {
    "name": "The Nexus Embassy",
    "email": "TheNexusEmbassy@nexus.io",
    "url": "https://github.com/Nexusoft"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Nexusoft/NexusInterface/issues"
  },
  "keywords": [
    "electron",
    "react",
    "redux",
    "sass",
    "webpack"
  ],
  "homepage": "https://github.com/Nexusoft/NexusInterface/#readme",

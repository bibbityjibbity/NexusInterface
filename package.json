    "webpack-dev-server": "^2.11.3",
    "webpack-merge": "^4.1.4",
    "webpack-sources": "^1.1.0"
  },
  "dependencies": {
    "ajv": "^6.5.4",
    "alert-node": "^1.2.2",
    "async": "^2.6.1",
    "auto-launch": "^5.0.5",
    "copy-to-clipboard": "^3.0.8",
    "csvtojson": "^2.0.8",
    "devtron": "^1.4.0",
    "electron": "^3.0.0",
    "electron-debug": "^1.2.0",
    "electron-directory": "0.0.3",
    "electron-log": "^2.2.17",
    "electron-updater": "^3.1.1",
    "find-process": "^1.1.3",
    "font-awesome": "^4.7.0",
    "getmac": "^1.4.6",
    "history": "^4.6.3",

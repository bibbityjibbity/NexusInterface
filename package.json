{
  "name": "nexus-tritium-beta",
  "productName": "Nexus_Tritium_Wallet_Beta",
  "version": "0.8.1",
  "buildDate": "October 19th 2018",
  "description": "Nexus_Tritium_Wallet_Beta",
  "scripts": {
    "build": "concurrently \"npm run build-main\" \"npm run build-renderer\"",
    "build-dll": "cross-env NODE_ENV=development node --trace-warnings -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.renderer.dev.dll.js --color",
    "build-main": "cross-env NODE_ENV=production node --trace-warnings -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.main.prod.js --color",
    "build-renderer": "cross-env NODE_ENV=production node  --max-old-space-size=4076 --trace-warnings -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.renderer.prod.js --color",
    "dev": "cross-env START_HOT=1 npm run start-renderer-dev",
    "lint": "eslint --cache --format=node_modules/eslint-formatter-pretty .",
    "lint-fix": "npm run lint -- --fix",
    "lint-styles": "stylelint app/*.css app/components/*.css --syntax scss",
    "lint-styles-fix": "stylefmt -r app/*.css app/components/*.css",
    "package": "npm run build && build --publish never",
    "package-all": "npm run build && build -mwl",
    "package-linux": "npm run build && build --linux",
    "package-mac": "npm run build && build --mac",
    "package-win": "npm run build && build --win --x64",
    "prestart": "npm run build",
    "start": "cross-env NODE_ENV=production electron ./app/",
    "start-main-dev": "cross-env HOT=1 NODE_ENV=development electron -r babel-register ./app/main.dev",
    "start-renderer-dev": "cross-env NODE_ENV=development node --trace-warnings -r babel-register ./node_modules/webpack-dev-server/bin/webpack-dev-server --config webpack.config.renderer.dev.js",
    "test": "cross-env NODE_ENV=test BABEL_DISABLE_CACHE=1 node --trace-warnings ./test/runTests.js",
    "test-e2e": "cross-env NODE_ENV=test BABEL_DISABLE_CACHE=1 node --trace-warnings ./test/runTests.js e2e",
    "test-watch": "npm test -- --watch",
    "version2": "node ./internals/scripts/UpdateBuildDate.js"
  },
  "browserslist": "electron 1.8.8",
  "build": {
    "productName": "Nexus_Tritium_Wallet_Beta",
    "appId": "com.nexusearth.NexusTritiumBeta",
    "files": [
      "dist/",
      "node_modules/",
      "app.html",
      "main.prod.js",
      "main.prod.js.map",
      "package.json",
      "resources/**/*",
      "installerSiderbar.bmp"
    ],
    "extraResources": [
      "./app/GeoLite2-City/**/*",
      "./app/images/tray/**/*",
      "./app/cores/**/*",
      "./app/images/background/**/*",
      "license.txt",
      "installerSidebar.bmp"
    ],
    "publish": {
      "provider": "github"
    },
    "mac": {
      "target": "dmg",
      "category": "public.app-category.utilities"
    },
    "dmg": {
      "background": "resources/background.jpg",
      "contents": [
        {
          "x": 129,
          "y": 250
        },
        {
          "x": 410,
          "y": 247,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": [
        "nsis"
      ]
    },
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "runAfterFinish": false,
      "allowToChangeInstallationDirectory": true,
      "menuCategory": "Nexus",
      "installerSidebar": "resources/installerSidebar.bmp",
      "license": "resources/license.txt",
      "shortcutName": "Tritium Wallet Beta",
      "artifactName": "Nexus Tritium Wallet Beta Setup.exe"
    },
    "linux": {
      "target": [
        "deb"
      ]
    },
    "directories": {
      "buildResources": "resources",
      "output": "release"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Nexusoft/Nexus-Interface-React.git"
  },
  "author": {
    "name": "The Nexus Embassy",
    "email": "TheNexusEmbassy@nexus.io",
    "url": "https://github.com/Nexusoft"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Nexusoft/Nexus-Interface-React/issues"
  },
  "keywords": [
    "electron",
    "react",
    "redux",
    "sass",
    "webpack"
  ],
  "homepage": "https://github.com/Nexusoft/Nexus-Interface-React/#readme",
  "jest": {
    "moduleNameMapper": {
      "\\.(mmdb|jpg|jpeg|bmp|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/internals/mocks/fileMock.js",
      "\\.(css|less|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "js"
    ],
    "moduleDirectories": [
      "node_modules",
      "app/node_modules"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "setupFiles": [
      "./internals/scripts/CheckBuiltsExist.js"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-dynamic-import-webpack": "^1.1.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-classes": "^6.24.1",
    "babel-preset-env": "^1.5.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-app": "^3.1.2",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "babili-webpack-plugin": "^0.1.2",
    "chalk": "^2.0.1",
    "concurrently": "^3.6.1",
    "copy-webpack-plugin": "^4.5.3",
    "cross-env": "^5.0.0",
    "cross-spawn": "^5.1.0",
    "css-loader": "^0.28.3",
    "electron": "^1.8.8",
    "electron-builder": "^20.28.4",
    "electron-devtools-installer": "^2.2.0",
    "enzyme": "^2.9.1",
    "enzyme-to-json": "^1.5.1",
    "eslint": "^5.7.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-formatter-pretty": "^1.1.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-plugin-compat": "^2.6.2",

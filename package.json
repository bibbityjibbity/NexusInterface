    "react-addons-test-utils": "^15.6.0",
    "react-test-renderer": "^15.6.1",
    "redux-logger": "^3.0.6",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "sinon": "^2.3.5",
    "spectron": "^3.7.0",
    "style-loader": "^0.18.1",
    "stylefmt": "^6.0.2",
    "stylelint": "^7.12.0",
    "stylelint-config-standard": "^16.0.0",
